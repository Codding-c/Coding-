//find hcf number
#include<stdio.h>
int main()
{
    int a, b;
    int hcf;
    printf("ENTER ANY TWO NUMBER: \n");
    scanf("%d%d",&a,&b);

    for(int i=1;i<=a || i<=b;i++){
        if(a%i==0 && b%i==0 )
        {
            hcf=i;
        }

        }
    printf("%d AND %d HCF NUMBER IS:%d \n",a,b,hcf);
   return 0;
    }

